def_key "j"
    scroll_down

def_key "k"
    scroll_up

def_key "h"
    previous_column

def_key "l"
    next_column

def_key "n"
    next_found_item

def_key "N"
    previous_found_item

def_key "D"
  delete_playlist_items

def_key "D"
  push_character "delete"
  push_characters "y"

def_key "D"
  delete_stored_playlist

