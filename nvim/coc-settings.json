{
     "coc.preferences.rootPatterns": [".git", "Cargo.toml", "package.json"],
     "coc.source.around.enable": false,
     "coc.source.buffer.enable": false,
     "coc.source.file.enable": false,
     "rust-analyzer.procMacro.enable": true,
     "rust-analyzer.cargo.loadOutDirsFromCheck": true,
     "rust-analyzer.completion.postfix.enable": false,
     "rust-analyzer.serverPath": "rust-analyzer",
     "explorer.icon.enableNerdfont": true,
     "explorer.previewAction.onHover": false,
     "explorer.keyMappingMode": "none",
     "explorer.width": 30,
     "explorer.file.root.template": "[icon] [root]",
     "explorer.sources": [{ "name": "file", "expand": true }],
     "explorer.file.showHiddenFiles": true,
     "explorer.keyMappings": {
          "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
          "a": "addFile",
          "A": "addDirectory",
          "r": "rename",
          "dd": "deleteForever",
          "?": "help",
          "q": "quit",
          "m": "actionMenu",
          "o": "open"
     },
     "diagnostic.errorSign": "",
     "diagnostic.warningSign": "⚠",
     "suggest.completionItemKindLabels": {
          "function": "\uf794",
          "method": "\uf6a6",
          "variable": "\uf71b",
          "constant": "\uf8ff",
          "struct": "\ufb44",
          "class": "\uf0e8",
          "interface": "\ufa52",
          "text": "\ue612",
          "enum": "\uf435",
          "enumMember": "\uf02b",
          "module": "\uf668",
          "color": "\ue22b",
          "property": "\ufab6",
          "field": "\uf93d",
          "unit": "\uf475",
          "file": "\uf471",
          "value": "\uf8a3",
          "event": "\ufacd",
          "folder": "\uf115",
          "keyword": "\uf893",
          "snippet": "\uf64d",
          "operator": "\uf915",
          "reference": "\uf87a",
          "typeParameter": "\uf278",
          "default": "\uf29c"
     }
}
